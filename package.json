{
  "name": "@dvhb/eslint-config",
  "version": "1.0.2-prettier.1",
  "description": "dvhb eslint config",
  "main": "index.js",
  "files": [
    "src",
    "index.js"
  ],
  "keywords": [
    "dvhb",
    "eslint"
  ],
  "scripts": {
    "test": "eslint .",
    "fix": "yarn test --fix",
    "print": "eslint --print-config .",
    "precommit": "lint-staged"
  },
  "author": "Aleksey Subbotenko <asu@dvhb.ru>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "7.1.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "prettier-eslint": "^8.8.2"
  },
  "devDependencies": {
    "eslint": "3.16.x",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
